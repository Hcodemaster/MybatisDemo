<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatis.mapper.UtilMapper">
    <insert id="insertToDb" useGeneratedKeys="true">
        replace into id_generate(name)
        values(#{name})
    </insert>

    <select id="getIdFromDb" resultType="java.lang.Integer">
        select max(id) from id_generate
        where name = #{name}
    </select>
</mapper>